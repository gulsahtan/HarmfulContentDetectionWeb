@using HarmfulContentDetection.Web.Startup
@{ ViewBag.CurrentPageName = PageNames.About;
    ViewBag.Title = L("About"); }
<div class="content-header">
    <div class="container-fluid">
        <div class="row">
            <div class="col-sm-12">
                <h1 class="m-0 text-dark">@L("About")</h1>
            </div>
        </div>
    </div>
</div>
<div class="content">
    <div class="container-fluid">
        <div class="row">
            <div class="col-lg-12">
                <div class="card">
                    <div class="card-body">
                        <p>
                            @L("Intro")
                        </p>

                        <h3>@L("MenusDo")</h3>

                        <p>
                            <strong>
                                @L("ObjectDetectionImage")
                            </strong> @L("ObjectDetectionImageAbout")

                        </p>

                        <p>
                            <strong>@L("ObjectDetectionVideo") </strong> @L("ObjectDetectionVideoAbout")
                        </p>


                        <p>
                            <strong>@L("ObjectDetectionRealTime") </strong> @L("ObjectDetectionRealTimeAbout")
                        </p>
                        <p>
                            <strong>@L("DataSet")  </strong> @L("DataSetAbout")
                        </p>
                        <p>
                            <strong>@L("VideoWatchingPlatform")  </strong> @L("VideoWatchingPlatformAbout")
                        </p>
                        @*<p>
                            <strong>@L("VideoInsertPlatform")  </strong> @L("VideoInsertPlatformAbout")
                        </p>*@
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
